{
    "manifest_version": 2,
    "name": "Remove Twitch Cheer",
    "version": "1.0",
  
    "description": "Adds a red border to all webpages matching mozilla.org.",
  
    "icons": {
      "48": "icons/icon-48.png",
      "96": "icons/icon-96.png"
    },
  
    "content_scripts": [
      {
        "matches": ["https://www.twitch.tv/*"],
        "js": ["remove_twitch_cheer.js"]
      }
    ]
  }
  